{
  "name": "rector-laravel-custom-rules/rules",
  "description": "Custom Rector rules for laravel",
  "type": "library",
  "license": "MIT",
  "authors": [
    {
      "name": "Jingtai Piao",
      "email": "piaojingtai@gmail.com"
    }
  ],
  "require": {
    "php": ">=8.3"
  },
  "require-dev": {
    "phpunit/phpunit": "^12.3",
    "rector/rector": "^2.1",
    "laravel/pint": "^1.24"
  },
  "autoload": {
    "psr-4": {
      "RectorLaravelCustomRules\\": "src/"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "RectorLaravelCustomRules\\Tests\\": "tests/"
    },
    "files": [
      "vendor/rector/rector/preload.php"
    ]
  },
  "scripts": {
    "check:lint": "vendor/bin/pint --test",
    "test": "vendor/bin/phpunit"
  }
}
