{
  "name": "rector-lara/laravel-eloquent-generic-rector",
  "description": "Custom Rector rule to add generics to Laravel Eloquent relationships",
  "type": "library",
  "license": "MIT",
  "version": "1.1.0",
  "authors": [
    {
      "name": "Jingtai Piao",
      "email": "piaojingtai@gmail.com"
    }
  ],
  "require": {
    "php": ">=8.3"
  },
  "require-dev": {
    "phpunit/phpunit": "^12.3",
    "rector/rector": "^1.2"
  },
  "autoload": {
    "psr-4": {
      "RectorLaravel\\Rules\\": "src/"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "RectorLaravel\\Tests\\": "tests/"
    },
    "files": [
      "vendor/rector/rector/preload.php"
    ]
  }
